<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.7.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='portal'>

	<inherits name='org.bonitasoft.console.AllBonitaConsole' />

	<!-- Limit localization permutations -->
	<set-property name="locale" value="default" />
	
	<!-- Limit permutations to firefox/chrmoe. Add ie7,ie8,ie9 to support internet explorer -->
	<set-property name="user.agent" value="gecko1_8,safari,ie8,ie9" />

    <!-- Enable gwt super dev mode -->
    <add-linker name="xsiframe"/>
    <set-configuration-property name="devModeRedirectEnabled" value="true"/>

</module>
